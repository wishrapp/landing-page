# Handle SPA routing
/* /index.html 200

# Handle image files with proper caching
/*.webp
  Cache-Control: public, max-age=31536000, immutable

/*.png
  Cache-Control: public, max-age=31536000, immutable

/*.jpg
  Cache-Control: public, max-age=31536000, immutable

/*.svg
  Cache-Control: public, max-age=31536000, immutable

# Fallback for other static assets
/assets/*
  Cache-Control: public, max-age=31536000, immutable
